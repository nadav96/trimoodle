{
	"name": "Getting Started Example",
	"description": "Build an Extension!",
	"version": "1.0",
	"manifest_version": 2,
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "Trim moodle"
	 },
	"permissions": [
		"webNavigation",
		"https://moodle2.cs.huji.ac.il/*",
		"tabs",
		"storage"
	],
	"content_scripts": [
		{
			"js": [
				"3party/jquery-3.6.0.min.js",
				"content.js"
			],
			"matches": [
				"https://moodle2.cs.huji.ac.il/*"
			]
		}
	]
}